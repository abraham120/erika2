CPU PerfTestApp {
  OS EE {
    EE_OPT = "EE_DEBUG";

    EE_OPT = "EE_EXECUTE_FROM_RAM";
    EE_OPT = "EE_SAVE_TEMP_FILES";
    //EE_OPT = "EE_MM_OPT";

    MEMORY_PROTECTION = TRUE;
    STACKMONITORING = TRUE;
    /* Un flag va generato per questa opzione */
    EE_OPT = "EE_AS_SERVICE_PROTECTION__";

    /* Used to enable static map of OS-Applications on protection sets
       optimization */
    EE_OPT = "EE_OS_APP_MAPPED_ON_PROTECTION_SETS";

    CPU_DATA = TRICORE {
      CPU_CLOCK = 200.0;
      APP_SRC = "code.c";
      APP_SRC = "app1.c";
      APP_SRC = "app2.c";
      APP_SRC = "trusted.c";
      COMPILER_TYPE = GNU;
      MULTI_STACK = TRUE;
    };

    MCU_DATA = TRICORE {
      MODEL = TC26x;
    };

    STATUS = EXTENDED;
    USEPARAMETERACCESS = FALSE;
    USERESSCHEDULER = FALSE;

    KERNEL_TYPE = BCC1;

    ORTI_SECTIONS = ALL;
  };

  TASK MainTask {
    PRIORITY = 10;
    ACTIVATION = 1;
    SCHEDULE = FULL;
    AUTOSTART = TRUE;
    STACK = SHARED;
  };

  TASK TrustedTask1 {
    PRIORITY = 1;
    ACTIVATION = 1;
    SCHEDULE = FULL;
    AUTOSTART = FALSE;
    STACK = SHARED;
  };

  TASK ErrorTask {
    PRIORITY = 100;
    ACTIVATION = 1;
    SCHEDULE = NON;
    AUTOSTART = FALSE;
    STACK = SHARED;
  };

  TASK App1Init {
    PRIORITY = 2;
    ACTIVATION = 1;
    SCHEDULE = FULL;
    AUTOSTART = FALSE;
    STACK = SHARED;
  };

  TASK App1Task {
    PRIORITY = 4;
    ACTIVATION = 1;
    SCHEDULE = FULL;
    AUTOSTART = FALSE;
    STACK = SHARED;
  };

  TASK App1Ack {
    PRIORITY = 10;
    ACTIVATION = 1;
    SCHEDULE = FULL;
    AUTOSTART = FALSE;
    STACK = SHARED;
  };

  TASK App1Bkg {
    PRIORITY = 1;
    ACTIVATION = 1;
    SCHEDULE = FULL;
    AUTOSTART = FALSE;
    STACK = PRIVATE {
      SYS_SIZE = 512;
    };
  };

  TASK App2Init {
    PRIORITY = 2;
    ACTIVATION = 1;
    SCHEDULE = FULL;
    AUTOSTART = FALSE;
    STACK = SHARED;
  };

  TASK App2Task {
    PRIORITY = 5;
    ACTIVATION = 1;
    SCHEDULE = FULL;
    AUTOSTART = FALSE;
    STACK = PRIVATE {
      SYS_SIZE = 512;
    };
  };

  TASK App2Ack {
    PRIORITY = 10;
    ACTIVATION = 1;
    SCHEDULE = FULL;
    AUTOSTART = FALSE;
    STACK = SHARED;
  };

  TASK App2HiPriTask {
    PRIORITY = 1000;
    ACTIVATION = 1;
    SCHEDULE = FULL;
    AUTOSTART = FALSE;
    STACK = SHARED;
  };

  ISR App1Isr {
    CATEGORY = 2;
    PRIORITY = 1;
  };

  ISR App2Isr {
    CATEGORY = 2;
    PRIORITY = 2;
  };

  APPLICATION TrustedApp {
    TRUSTED = TRUE {
      TRUSTED_FUNCTION = TRUE {
        NAME = "MyTaskTrustedService";
      };
      TRUSTED_FUNCTION = TRUE {
        NAME = "MyIsrTrustedService";
      };
    };
    TASK = MainTask;
    TASK = TrustedTask1;
    TASK = ErrorTask;
    MEMORY_SIZE = 0x1000;
    SHARED_STACK_SIZE = 512;
    IRQ_STACK_SIZE = 512;
  };

  APPLICATION App1 {
    TRUSTED = FALSE;
    ISR = "App1Isr";
    TASK = App1Init;
    TASK = App1Task;
    TASK = App1Ack;
    TASK = App1Bkg;
    MEMORY_SIZE = 0x1000;
    SHARED_STACK_SIZE = 512;
    IRQ_STACK_SIZE = 512;
  };

  APPLICATION App2 {
    TRUSTED = FALSE;
    ISR = "App2Isr";
    TASK = App2Init;
    TASK = App2Task;
    TASK = App2Ack;
    TASK = App2HiPriTask;
    MEMORY_SIZE = 0x1000;
    SHARED_STACK_SIZE = 512;
    IRQ_STACK_SIZE = 512;
  };
};
